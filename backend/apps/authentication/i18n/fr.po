# Login page
msgid "Login"
msgstr "Connexion"

msgid "Email or username"
msgstr "Email ou nom d'utilisateur"

msgid "Password"
msgstr "Mot de passe"

msgid "Remember me"
msgstr "Se souvenir de moi"

msgid "Forgot password?"
msgstr "Mot de passe oublié ?"

msgid "Don't have an account?"
msgstr "Pas encore de compte ?"

msgid "Create an account"
msgstr "Créer un compte"

# Login messages
msgid "Welcome, %(username)s!"
msgstr "Bienvenue, %(username)s !"

msgid "Invalid username or password."
msgstr "Nom d'utilisateur ou mot de passe incorrect."

msgid "Sign in to your account"
msgstr "Connectez-vous à votre compte"

msgid "or"
msgstr "ou"

msgid "Back to home"
msgstr "Retour à l'accueil"

# Register page
msgid "Sign up"
msgstr "S'inscrire"

msgid "Username"
msgstr "Nom d'utilisateur"

msgid "Email"
msgstr "Email"

msgid "First name"
msgstr "Prénom"

msgid "Last name"
msgstr "Nom"

msgid "Native language"
msgstr "Langue maternelle"

msgid "Target language"
msgstr "Langue cible"

msgid "I agree to the"
msgstr "J'accepte les"

msgid "Terms and Conditions"
msgstr "Conditions d'utilisation"

msgid "Privacy Policy"
msgstr "Politique de confidentialité"

msgid "Already have an account?"
msgstr "Déjà un compte ?"

# Error messages
msgid "Invalid username or password"
msgstr "Nom d'utilisateur ou mot de passe invalide"

msgid "This email is already registered"
msgstr "Cet email est déjà enregistré"

msgid "This username is already taken"
msgstr "Ce nom d'utilisateur est déjà pris"

msgid "Username cannot be empty"
msgstr "Le nom d'utilisateur ne peut pas être vide"

msgid "Username must contain at least 3 characters"
msgstr "Le nom d'utilisateur doit contenir au moins 3 caractères"

msgid "Username cannot exceed 30 characters"
msgstr "Le nom d'utilisateur ne peut pas dépasser 30 caractères"

msgid "Username can only contain letters, numbers, _ and -"
msgstr "Le nom d'utilisateur ne peut contenir que des lettres, chiffres, _ et -"

msgid "Username must contain at least one letter or number"
msgstr "Le nom d'utilisateur doit contenir au moins une lettre ou un chiffre"

msgid "This username is available"
msgstr "Ce nom d'utilisateur est disponible"

msgid "Native language and target language cannot be the same"
msgstr "La langue native et la langue cible ne peuvent pas être identiques"

msgid "Checking availability..."
msgstr "Vérification en cours..."

msgid "Error during verification"
msgstr "Erreur lors de la vérification"

msgid "Passwords do not match"
msgstr "Les mots de passe ne correspondent pas"

# Registration form fields
msgid "Create your free account"
msgstr "Créez votre compte gratuit"

msgid "Your first name"
msgstr "Votre prénom"

msgid "Your last name"
msgstr "Votre nom"

msgid "username"
msgstr "Nom d'utilisateur"

msgid "Email address"
msgstr "Adresse email"

msgid "your@email.com"
msgstr "votre@email.com"

msgid "Phone number"
msgstr "Numéro de téléphone"

msgid "Optional - for account recovery"
msgstr "Optionnel - pour la récupération de compte"

msgid "Date of birth"
msgstr "Date de naissance"

msgid "Gender"
msgstr "Genre"

msgid "Select your gender"
msgstr "Sélectionnez votre genre"

msgid "Male"
msgstr "Homme"

msgid "Female"
msgstr "Femme"

msgid "Other"
msgstr "Autre"

msgid "Prefer not to say"
msgstr "Préfère ne pas dire"

msgid "Confirm password"
msgstr "Confirmer le mot de passe"

msgid "Your native language"
msgstr "Votre langue native"

