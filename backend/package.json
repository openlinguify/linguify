{
  "name": "linguify-frontend",
  "version": "1.0.0",
  "description": "Frontend assets for Linguify platform with OWL",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "serve": "webpack serve --mode development",
    "generate-templates": "node scripts/generate-templates.js",
    "build:language-learning": "npm run generate-templates && webpack --config webpack.config.language-learning.js --mode production",
    "dev:language-learning": "npm run generate-templates && webpack --config webpack.config.language-learning.js --mode development --watch"
  },
  "keywords": [
    "owl",
    "django",
    "language-learning"
  ],
  "author": "OpenLinguify",
  "license": "MIT",
  "dependencies": {
    "@odoo/owl": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^5.2.2",
    "xmldom": "^0.6.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
